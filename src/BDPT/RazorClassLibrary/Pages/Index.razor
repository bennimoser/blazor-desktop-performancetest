@page "/"
@using ClassLibrary

@code {

    [Inject]
    public NavigationManager NavManager { get; set; }

    protected override void OnAfterRender(bool firstRender)
    {
        if (firstRender)
        {
            NavManager.NavigateTo($"studentoverview/{AmountMeasurements}");
        }
        base.OnAfterRender(firstRender);
    }

    public int AmountMeasurements => 100;
}
